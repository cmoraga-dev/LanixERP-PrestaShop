<?php


namespace Lanix;
use JMS\Serializer\Annotation as Serializer;
class SimplePersona
{
    /** @Serializer\Type ("string") */
    private $rut;

    /** @Serializer\Type ("string") */
    private $grupo;

    /** @Serializer\Type ("string") */
    private $giro;

    /** @Serializer\Type ("string") */
    private $razonSocial;

    /** @Serializer\Type ("string") */
    private $telefono;

    /** @Serializer\Type ("string") */
    private $email;

    /** @Serializer\Type ("string") */
    private $codPais;

    /** @Serializer\Type ("string") */
    private $codRegion;

    /** @Serializer\Type ("string") */
    private $codCiudad;

    /** @Serializer\Type ("string") */
    private $codComuna;

    /** @Serializer\Type ("string") */
    private $direccion;

    /**
     * @return mixed
     */
    public function getRut()
    {
        return $this->rut;
    }

    /**
     * @param mixed $rut
     */
    public function setRut($rut)
    {
        $this->rut = $rut;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getRazonSocial()
    {
        return $this->razonSocial;
    }

    /**
     * @param mixed $razonSocial
     */
    public function setRazonSocial($razonSocial)
    {
        $this->razonSocial = $razonSocial;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getTelefono()
    {
        return $this->telefono;
    }

    /**
     * @param mixed $telefono
     */
    public function setTelefono($telefono)
    {
        $this->telefono = $telefono;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getEmail()
    {
        return $this->email;
    }

    /**
     * @param mixed $email
     */
    public function setEmail($email)
    {
        $this->email = $email;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getCodPais()
    {
        return $this->codPais;
    }

    /**
     * @param mixed $codPais
     */
    public function setCodPais($codPais)
    {
        $this->codPais = $codPais;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getCodRegion()
    {
        return $this->codRegion;
    }

    /**
     * @param mixed $codRegion
     */
    public function setCodRegion($codRegion)
    {
        $this->codRegion = $codRegion;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getCodCiudad()
    {
        return $this->codCiudad;
    }

    /**
     * @param mixed $codCiudad
     */
    public function setCodCiudad($codCiudad)
    {
        $this->codCiudad = $codCiudad;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getCodComuna()
    {
        return $this->codComuna;
    }

    /**
     * @param mixed $codComuna
     */
    public function setCodComuna($codComuna)
    {
        $this->codComuna = $codComuna;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getDireccion()
    {
        return $this->direccion;
    }

    /**
     * @param mixed $direccion
     */
    public function setDireccion($direccion)
    {
        $this->direccion = $direccion;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getGiro()
    {
        return $this->giro;
    }

    /**
     * @param mixed $giro
     * @return SimplePersona
     */
    public function setGiro($giro)
    {
        $this->giro = $giro;
        return $this;
    }

    /**
     * @return mixed
     */
    public function getGrupo()
    {
        return $this->grupo;
    }

    /**
     * @param mixed $grupo
     * @return SimplePersona
     */
    public function setGrupo($grupo)
    {
        $this->grupo = $grupo;
        return $this;
    }

}
